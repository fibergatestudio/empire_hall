      </div>
      {% if categories %}
        <div class="shop-menu">
          <div class="menu__close">
            <svg><use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="front/dist/images/sprite.svg#close"></use></svg>
          </div>
          <div class="shop-menu__inner">
            <ul class="shop-menu__list mCustomScrollbar first-level">
              {% for category in categories %}
                {% for child in category.children %}
                    <li class="shop-menu__item">
                        {% if child.children_third %}
                            <a href="{{ child.href }}" class="has-submenu" data-sub="sub-{{ child.category_id }}">
                              <span>{{ child.name }}</span>
                              <svg>
                                <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="front/dist/images/sprite.svg#opener"></use>
                              </svg>
                            </a>
                        {% else %}
                            <li class="shop-menu__item">
                              <a href="{{ child.href }}">{{ child.name }}</a>
                            </li>
                        {% endif %}
                    </li>
                {% endfor %}
              {% endfor %}
            </ul>
            {% for category in categories %}
              {% for child in category.children %}
                {% if child.children_third %}
                  <ul class="shop-menu__list mCustomScrollbar sub second-level sub-{{ child.category_id }}">
                    <li class="shop-menu__item shop-menu__back">
                      <a href="#">
                        <svg>
                          <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="front/dist/images/sprite.svg#opener"></use>
                        </svg>
                        Назад
                      </a>
                    </li>
                    {% for child2 in child.children_third %}
                      {% if child2.children_fourth %}
                        <li class="shop-menu__item">
                          <a class="has-submenu" data-sub="sub-{{ child2.category_id }}" href="{{ child2.href }}">
                            <span>{{ child2.name }}</span>
                            <svg>
                              <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="front/dist/images/sprite.svg#opener"></use>
                            </svg>
                          </a>
                        </li>
                      {% else %}
                        <li class="shop-menu__item">
                          <a href="{{ child2.href }}">{{ child2.name }}</a>
                        </li>
                      {% endif %}
                    {% endfor %}
                  </ul>
                {% endif %}
              {% endfor %}
            {% endfor %}
            {% for category in categories %}
              {% for child in category.children %}
                {% if child.children_third %}
                    {% for child2 in child.children_third %}
                      {% if child2.children_fourth %}
                        <ul class="shop-menu__list mCustomScrollbar sub third-level sub-{{ child2.category_id }}">
                          <li class="shop-menu__item shop-menu__back">
                            <a href="#">
                              <svg>
                                <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="front/dist/images/sprite.svg#opener"></use>
                              </svg>
                              Назад
                            </a>
                          </li>
                          {% for child3 in child2.children_fourth %}
                                <li class="shop-menu__item">
                                  <a href="{{ child3.href }}">
                                    <span>{{ child3.name }}</span>
                                  </a>
                                </li>
                          {% endfor %}
                        </ul>
                      {% endif %}
                    {% endfor %}
                {% endif %}
              {% endfor %}
            {% endfor %}
          </div>
        </div>
      {% endif %}
    </header>
